{
	"manifest_version": 2,
	"name": "__MSG_appName__",
	"description": "__MSG_appDesc__",
	"version": "0.0.2",
    "default_locale": "en",
	"background": {
		"scripts": ["background.js"],
        "persistent": true
	},
	"browser_action": {
		"default_icon": "locked128.png",
		"default_title": "Site Unblocker"
	},
	"content_scripts": [{
			"js": ["contentScript.js"],
			"matches": ["https://*/*", "http://*/*"]
		}
	],
	"icons": {
		"128": "unlocked128.png"
	},
    "permissions": [
        "alarms",
        "webRequest",
        "webRequestBlocking",
        "\u003Call_urls>",
        "storage",
        "tabs"
    ]
}
